   "source": [
    "import numpy as np\n",
    "\n",
    "num=1024\n",
    "count=0\n",
    "\n",
    "while np.sqrt(num)%1:\n",
    "    num-=1\n",
    "    count+=1\n",
    "pos=(np.sqrt(num)-1)/2\n",
    "\n",
    "while count>pos:\n",
    "    count=count-pos\n",
    "\n",
    "print(abs(pos)*2-count)"
   ]
  }
